{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [ "dom", "dom.iterable", "esnext" ],
    "module": "es2020",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "rootDir": "app/javascript/turbo",
    "strict": true,
    "target": "es2017",
    "noEmit": true,
    "skipLibCheck": true,
    "removeComments": false
  },
  "exclude": [ "dist" ]
}
